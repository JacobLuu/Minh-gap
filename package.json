  "scripts": {
    "postinstall": "husky install",
    "analyze": "source-map-explorer build/static/js/main.*",
    "start:gap-admin": "yarn install && FRONTEND_PACKAGE=gap-admin node scripts/start.js",
    "build:gap-admin": "yarn install && FRONTEND_PACKAGE=gap-admin node scripts/build.js",
    "lint:gap-admin": "yarn install && eslint 'packages/gap-admin/src/{components,constants,containers,context,helpers,reducers,services,store,utils}/**/*.{tsx,ts}' --fix",
    "start:gap-frontend": "yarn install && FRONTEND_PACKAGE=gap-frontend node scripts/start.js",
    "build:gap-frontend": "yarn install && FRONTEND_PACKAGE=gap-frontend node scripts/build.js",
    "lint:gap-frontend": "yarn install && eslint 'packages/gap-frontend/src/{components,constants,containers,context,helpers,reducers,services,store,utils}/**/*.{tsx,ts}' --fix",
    "lint:frontend": "yarn install && eslint 'packages/frontend/src/{components,constants,containers,context,helpers,reducers,services,store,utils}/**/*.{tsx,ts}' --fix",
    "lint:gap-common": "yarn install && eslint 'packages/gap-common/src/{components,constants,containers,context,helpers,reducers,services,store,utils}/**/*.{tsx,ts}' --fix",
    "test": "yarn install && node scripts/test.js --env=jsdom",
    "test:ci": "yarn install && CI=true node scripts/test.js --env=jsdom",
    "test:frontend": "yarn install && node scripts/test.js --testPathPattern=\"./packages/frontend/src/\" --env=jsdom",
    "test:admin": "yarn install && node scripts/test.js --testPathPattern=\"./packages/admin/src/\" --env=jsdom",
    "eslint": "eslint --ext js,ts,tsx",
    "generate": "plop --plopfile generators/index.js",
    "storybook": "start-storybook -p 6006",
    "test-storybook": "test-storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config-file $CYPRESS_CONFIG",
    "e2e-test:gap-admin": "npx cypress run --config-file ./packages/gap-admin/e2e-tests/configs/config.ts",
    "e2e-test:watch:gap-admin": "npx cypress open --config-file ./packages/gap-admin/e2e-tests/configs/config.ts",
    "e2e-test:gap-frontend": "npx cypress run --config-file ./packages/gap-frontend/e2e-tests/configs/config.ts",
    "e2e-test:watch:gap-frontend": "npx cypress open --config-file ./packages/gap-frontend/e2e-tests/configs/config.ts",
    "translate": "yarn json-autotranslate --source-language=en --directory-structure=default --input=packages/gap-frontend/src/locales"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}$": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "*.stories.@(ts|tsx|js|jsx|mjs|cjs)": [
      "test-storybook"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
